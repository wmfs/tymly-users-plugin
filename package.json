{
  "name": "@wmfs/tymly-users-plugin",
  "version": "0.0.0-semantically-released",
  "description": "Plugin which handles interactions to do with a user for Tymly framework",
  "homepage": "https://github.com/wmfs/tymly-users-plugin#readme",
  "author": "West Midlands Fire Service",
  "keywords": [
    "tymly",
    "plugin",
    "user",
    "interaction"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wmfs/tymly-users-plugin.git"
  },
  "bugs": {
    "url": "https://github.com/wmfs/tymly-users-plugin/issues"
  },
  "main": "./lib/index.js",
  "dependencies": {
    "debug": "3.1.0",
    "@wmfs/form-maker": "1.0.1",
    "jsonfile": "4.0.0",
    "jsonpath": "1.0.0",
    "lodash": "4.17.10",
    "shasum": "1.0.2"
  },
  "devDependencies": {
    "chai": "4.1.2",
    "codecov": "3.0.3",
    "cz-conventional-changelog": "2.1.0",
    "mocha": "5.2.0",
    "nyc": "12.0.2",
    "semantic-release": "^15.5.2",
    "standard": "11.0.1",
    "@wmfs/hl-pg-client": "1.1.3",
    "@wmfs/tymly": "1.3.0",
    "@wmfs/tymly-pg-plugin": "1.3.0",
    "@wmfs/tymly-solr-plugin": "1.0.5"
  },
  "scripts": {
    "lint": "standard",
    "test": "nyc mocha && standard",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "analyzeCommits": {
      "preset": "angular",
      "releaseRules": [
        {
          "type": "build",
          "scope": "deps",
          "release": "minor"
        },
        {
          "type": "build",
          "scope": "deps-dev",
          "release": "patch"
        }
      ]
    }
  }
}
